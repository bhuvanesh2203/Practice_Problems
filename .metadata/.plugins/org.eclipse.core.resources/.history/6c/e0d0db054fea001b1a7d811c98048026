package Javain;



public class Consecutive1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           int n=15;
           int sum=0;
           int start=1;
           int end =(n+1)/2;
           while(start<end) {
        	   for(int i=sum;i<end;i++) {
        		   sum=sum+i;
        		   if(sum==n) {
        			   for(int j=start;j<i;j++) {
        				   System.out.println(j +" ");
        			   }
        			   
        			  System.out.println();
        			   break;
        		   }
        		   if(sum>n) {
        			   break;
        		   }
        		   
        	   }
        	   sum=0;
    		   start++;
        	   
        	   
           }
	}

}
