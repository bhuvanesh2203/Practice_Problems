	public static void main(String[] args) {
		// static or run time polimorphism
		/*
		 * Honda c =new Honda(); c.start(); c.stop(); c.reliability();
		 */
		
		
		ComplietheCars aCars=new ComplietheCars();
		
		System.out.println("-----");
		ComplietheCars aCars2=new ComplietheCars("String");
		
		System.out.println("-----");
		
	
		
		
		System.out.println(" next  child class oject is referreed by parent class its dynamic or run time polimorphism");
         Car p = new Honda();
         p.start();
