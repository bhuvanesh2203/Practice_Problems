package DataStructure;

public class CustomStack {

    public static final int DEFAULT_SIZE=10;
    protected  int data[];
    protected int ptr=-1;    
    public  CustomStack() {  
        this(DEFAULT_SIZE);
     
    }
    public CustomStack(int size) {
        this.data=new int [size];
        
    }
    public boolean isFull() {
        if(ptr==this.data.length-1) {
            return false;
        }
        return true;
    }
}
